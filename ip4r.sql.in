CREATE OR REPLACE FUNCTION ip4r_send(ip4r)
RETURNS bytea
AS 'MODULE_PATHNAME'
LANGUAGE 'C' IMMUTABLE STRICT;

CREATE TYPE ip4r (
	INTERNALLENGTH = 8,
	INPUT = ip4r_in,
	OUTPUT = ip4r_out,
	RECEIVE = ip4r_recv,
	SEND = ip4r_send,
	ALIGNMENT = int4,
        ELEMENT = ip4
);

COMMENT ON TYPE ip4r IS
'IPv4 range ''#.#.#.#-#.#.#.#'' or ''#.#.#.#/#'' or ''#.#.#.#''';

COMMIT;

